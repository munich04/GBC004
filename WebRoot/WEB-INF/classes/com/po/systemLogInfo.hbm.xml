<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.po.SystemLogInfo" table="systemloginfo">
		<id name="id" column="id">
			<generator class="uuid"></generator>
		</id>
		<property name="infoType" column="infoType"></property>
		<property name="userInfoId" column="userInfoId"></property>
		<property name="infoDetail" column="infoDetail"></property>
		<property name="recordTime" column="recordTime"></property>
	</class>

</hibernate-mapping>