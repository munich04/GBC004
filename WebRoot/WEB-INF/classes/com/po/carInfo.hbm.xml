<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.po.CarInfo" table="carinfo">
		<id name="id" column="id">
			<generator class="uuid"></generator>
		</id>
		<property name="carIDCardID" column="carIDCardID"></property>
		<property name="carDept" column="carDept"></property>
		<property name="carNumber" column="carNumber"></property>
		<property name="carDriver" column="carDriver"></property>
		<property name="tareWeight" column="tareWeight"></property>
		<property name="recordTime" column="recordTime"></property>
		<property name="carNotes" column="carNotes"></property>
	</class>

</hibernate-mapping>