<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.po.HangUpOperation" table="hangupoperation">
		<id name="id" column="id">
			<generator class="uuid"></generator>
		</id>
		<property name="dailyInfoID" column="dailyInfoID"></property>
		<property name="operateTime" column="operateTime"></property>
		<property name="operateNotes" column="operateNotes"></property>
		<property name="hangUpState" column="hangUpState"></property>
		<property name="operator" column="operator"></property>
	</class>

</hibernate-mapping>